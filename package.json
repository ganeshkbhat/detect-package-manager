{
  "name": "detect-package-manager",
  "version": "1.0.0",
  "description": "detect default package manager in nodejs using detect-package-manager",
  "main": "index.js",
  "exports": {
    "require": "./index.js",
    "import": "./index.mjs"
  },
  "unpkg": "index.min.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ganeshkbhat/detect-package-manager.git"
  },
  "bugs": {
    "url": "https://github.com/ganeshkbhat/detect-package-manager/issues"
  },
  "homepage": "https://github.com/ganeshkbhat/detect-package-manager#readme",
  "scripts": {
    "test": "mocha --reporter spec --recursive --timeout 60000",
    "uglify": "uglifyjs --compress --mangle reserved=['$','require','exports'] index.js -o index.min.js",
    "gitadd": "git add ."
  },
  "pre-commit": {
    "run": [
      "test"
    ]
  },
  "keywords": [],
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@posva/prompts": "^2.4.4",
    "@types/fs-extra": "^11.0.4",
    "@types/ini": "^1.3.33",
    "@types/which": "^3.0.3",
    "chai": "^4.4.1",
    "mocha": "^10.2.0",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.4",
    "sinon": "^17.0.1",
    "typescript": "^5.3.3",
    "uglify-js": "^3.17.4",
    "unimported": "^1.31.1"
  },
  "dependencies": {
    "find-up": "^6.3.0",
